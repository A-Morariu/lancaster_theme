\NeedsTeXFormat{LaTeX2e}
\ProvidesPackage{beamercolorthemelancaster}[2017/01/23 Lancaster color theme]
\RequirePackage{pgfopts}
\pgfkeys{
  /lancaster/color/block/.cd,
    .is choice,
    transparent/.code=\lancaster@block@transparent,
    fill/.code=\lancaster@block@fill,
}
\pgfkeys{
  /lancaster/color/background/.cd,
    .is choice,
    dark/.code=\lancaster@colors@dark,
    light/.code=\lancaster@colors@light,
}
\newcommand{\lancaster@color@setdefaults}{
  \pgfkeys{/lancaster/color/.cd,
    background=light,
    block=transparent,
  }
}

% Define colours
%Primary
\definecolor{lancasterGrey}{RGB}{190,192,194}
\definecolor{lancasterBodyGrey}{RGB}{85,86,86}
\definecolor{lancasterRed}{RGB}{181,18,27}
\definecolor{lancasterBlack}{RGB}{35,31,32}
\definecolor{lancasterLightGrey}{RGB}{233,236,237}

%Secondary
\definecolor{lancasterDarkGreen}{RGB}{85,120,105}
\definecolor{lancasterLightGreen}{RGB}{134,153,120}
\definecolor{lancasterBeige}{RGB}{186,182,162}
\definecolor{lancasterDarkGrey}{RGB}{50,65,71}
\definecolor{lancasterLightBlue}{RGB}{127,170,190}
\definecolor{lancasterLightPurple}{RGB}{100,96,108}
\definecolor{lancasterLightRed}{RGB}{194,103,99}
\definecolor{lancasterLightYellow}{RGB}{227,203,139}
\definecolor{lancasterLightOrange}{RGB}{225,171,108}


\definecolor{mDarkBrown}{HTML}{604c38}
\definecolor{mDarkTeal}{HTML}{23373b}
\definecolor{mLightBrown}{HTML}{EB811B}
\definecolor{mLightGreen}{HTML}{14B03D}
\newcommand{\lancaster@colors@dark}{
  \setbeamercolor{normal text}{%
    fg=black,
    bg=mDarkTeal
  }
  \usebeamercolor[fg]{normal text}
}
\newcommand{\lancaster@colors@light}{
  \setbeamercolor{normal text}{%
    fg=black,
    bg=%black!20
  }
}
\setbeamercolor{alerted text}{%
  fg=lancasterLightRed
}
\setbeamercolor{example text}{%
  fg=lancasterDarkGreen
}
\setbeamercolor{titlelike}{use=normal text, parent=normal text, fg=lancasterRed}


\newcommand{\lancaster@colors@white}{
  \setbeamercolor{title}{%
    use=normal text,
    parent=normal text, 
    fg=lancasterRed,
    bg=,
  }
  \setbeamercolor{titlebackground}{%
    bg=white,
  }
}
\newcommand{\lancaster@colors@red}{
  \setbeamercolor{title}{%
    use=normal text,
    parent=normal text,
    fg=white,
    bg=,
  }
  \setbeamercolor{author}{%
    fg=white,
    bg=,
  }
  \setbeamercolor{titlebackground}{%
    bg=lancasterRed,
  }
}
\newcommand{\lancaster@colors@blue}{
  \setbeamercolor{title}{%
    use=normal text,
    parent=normal text,
    fg=white,
    bg=,
  }
  \setbeamercolor{author}{%
    use=normal text,
    parent=normal text,
    fg=white,
    bg=,
  }
  \setbeamercolor{titlebackground}{%
    bg=lancasterLightBlue,
  }
}
\setbeamercolor{subtitle}{use=title}
\setbeamercolor{date}{use=author, parent=author}
\setbeamercolor{institute}{use=author, parent=author}
\setbeamercolor{structure}{use=normal text, fg=normal text.fg}
\setbeamercolor{palette primary}{%
  use=normal text,
  fg=normal text.bg,
  bg=normal text.fg
}
\setbeamercolor{frametitle}{%
  use=palette primary,
  parent=palette primary,
  fg=lancasterRed,
  bg=%lancasterGrey!15 %for debugging
}
\setbeamercolor{framesubtitle}{%
  use=palette primary,
  parent=palette primary,
  fg=lancasterRed,
  bg=%lancasterGrey!15 %for debugging
}
\setbeamercolor{progress bar}{%
  use=alerted text,
  fg=alerted text.fg,
  bg=alerted text.fg!50!black!30
}
\setbeamercolor{title separator}{
  use=progress bar,
  parent=progress bar
}
\setbeamercolor{progress bar in head/foot}{%
  use=progress bar,
  parent=progress bar
}

\setbeamercolor{frame numbering}{%
  fg=lancasterGrey
}

\setbeamercolor{progress bar in section page}{
  use=progress bar,
  parent=progress bar
}
\newcommand{\lancaster@block@transparent}{
  \setbeamercolor{block title}{%
    use=normal text,
    fg=normal text.fg,
    bg=
  }
  \setbeamercolor{block body}{
    bg=
  }
}
\newcommand{\lancaster@block@fill}{
  \setbeamercolor{block title}{%
    use=normal text,
    fg=normal text.fg,
    bg=normal text.bg!80!fg
  }
  \setbeamercolor{block body}{
    use={block title, normal text},
    bg=block title.bg!50!normal text.bg
  }
}
\setbeamercolor{block title alerted}{%
    use={block title, alerted text},
    bg=block title.bg,
    fg=alerted text.fg
}
\setbeamercolor{block title example}{%
    use={block title, example text},
    bg=block title.bg,
    fg=example text.fg
}
\setbeamercolor{block body alerted}{use=block body, parent=block body}
\setbeamercolor{block body example}{use=block body, parent=block body}
\setbeamercolor{footnote}{fg=normal text.fg!90}
\setbeamercolor{footnote mark}{fg=.}
\setbeamercolor{bibliography entry author}{fg=, bg=}
\setbeamercolor{bibliography entry title}{fg=, bg=}
\setbeamercolor{bibliography entry location}{fg=, bg=}
\setbeamercolor{bibliography entry note}{fg=, bg=}
\lancaster@color@setdefaults
\ProcessPgfPackageOptions{/lancaster/color}
\mode<all>
\endinput
